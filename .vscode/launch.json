{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(Windows) Anexar",
      "type": "cppvsdbg",
      "request": "attach",
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "extension-output-ms-vscode.cpptools-#2-Avisos de Configuração de C",
      "program": "extension-output-ms-vscode.cpptools-#2-Avisos de Configuração de C/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}